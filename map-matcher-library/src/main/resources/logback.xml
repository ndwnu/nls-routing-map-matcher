<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<!-- Default: %date %level [%thread] %logger{10} [%file:%line] %msg%n -->
			<pattern>%date %level \(%file:%line\) %msg%n
			</pattern>
		</encoder>
	</appender>

	<!-- Special treatment -->
	<logger name="com.graphhopper.matching.MapMatching" level="info" />
	<root level="info">
		<appender-ref ref="STDOUT" />
	</root>

</configuration>
